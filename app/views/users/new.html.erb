<h2>New user</h2>
<% if user_signed_in? %>
  <% if current_user.admin? && session[:last_url]==session[:url] %>
    <%= link_to 'Back', root_path, :class => "pull-right btn btn-primary btn-lg", :role =>"button" %><br/><br/>
  <% else %>
    <%= link_to 'Back', session[:last_url], :class => "pull-right btn btn-primary btn-lg", :role =>"button"%><br/><br/> 
  <% end %>
<% end %>
<div class="well well-lg">
  <%= render_breadcrumbs%>
  <%= render 'form' %>
</div><br/>
