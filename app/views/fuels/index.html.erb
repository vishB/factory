<h2>Listing fuels</h2>
<%= link_to 'New Fuel', new_fuel_path %><br/><%= link_to 'Back', root_path, :class => "pull-right" %><br/><br/>
<div class="well well-lg">
  <!-- Table -->
  <table class="table">
    <tr>
      <th>Fuel type</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>

    <% @fuels.each do |fuel| %>
      <tr>
        <td><%= fuel.fuel_type %></td>
        <td><%= link_to 'Show', fuel %></td>
        <td><%= link_to 'Edit', edit_fuel_path(fuel) %></td>
        <td><%= link_to 'Destroy', fuel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </table>
</div>

